{
  "metadata": {
    "version": "1.0.0",
    "created_at": "2025-01-17T00:00:00Z",
    "description": "ML Agent configurations for OMNI trading system"
  },
  "agents": {
    "quantum_predictor": {
      "id": "quantum_predictor_001",
      "name": "Quantum Market Predictor",
      "type": "quantum_ml",
      "enabled": true,
      "priority": 1,
      "config": {
        "model_type": "quantum_neural_network",
        "quantum_dimensions": 10000,
        "prediction_horizon": 24,
        "confidence_threshold": 0.75,
        "learning_rate": 0.001,
        "batch_size": 32,
        "epochs": 100,
        "features": [
          "price_momentum",
          "volume_profile",
          "volatility_surface",
          "market_microstructure",
          "quantum_entanglement"
        ]
      },
      "performance_targets": {
        "accuracy": 0.85,
        "precision": 0.80,
        "recall": 0.75,
        "f1_score": 0.77
      }
    },
    "hyperdimensional_pattern_recognizer": {
      "id": "hd_pattern_001",
      "name": "Hyperdimensional Pattern Recognizer",
      "type": "hyperdimensional_ml",
      "enabled": true,
      "priority": 2,
      "config": {
        "model_type": "hyperdimensional_computing",
        "vector_dimensions": 10000,
        "pattern_memory_size": 1000,
        "similarity_threshold": 0.7,
        "binding_operations": ["multiply", "permute", "bundle"],
        "learning_mode": "online",
        "features": [
          "candlestick_patterns",
          "support_resistance_levels",
          "trend_patterns",
          "reversal_signals",
          "continuation_patterns"
        ]
      },
      "performance_targets": {
        "pattern_recognition_accuracy": 0.80,
        "false_positive_rate": 0.15,
        "pattern_discovery_rate": 0.70
      }
    },
    "market_analyzer": {
      "id": "market_analyzer_001",
      "name": "Advanced Market Analyzer",
      "type": "ensemble_ml",
      "enabled": true,
      "priority": 3,
      "config": {
        "model_type": "ensemble",
        "base_models": [
          "random_forest",
          "gradient_boosting",
          "neural_network",
          "svm"
        ],
        "ensemble_method": "weighted_voting",
        "feature_selection": "recursive_elimination",
        "cross_validation_folds": 5,
        "features": [
          "technical_indicators",
          "market_sentiment",
          "order_book_imbalance",
          "news_sentiment",
          "social_media_sentiment"
        ]
      },
      "performance_targets": {
        "prediction_accuracy": 0.78,
        "market_timing_accuracy": 0.75,
        "trend_detection_accuracy": 0.82
      }
    },
    "sentiment_analyzer": {
      "id": "sentiment_analyzer_001",
      "name": "Multi-Source Sentiment Analyzer",
      "type": "nlp_ml",
      "enabled": true,
      "priority": 4,
      "config": {
        "model_type": "transformer",
        "base_model": "bert-base-uncased",
        "fine_tuned": true,
        "sentiment_sources": [
          "twitter",
          "reddit",
          "news_articles",
          "telegram",
          "discord"
        ],
        "sentiment_weights": {
          "twitter": 0.3,
          "reddit": 0.25,
          "news_articles": 0.25,
          "telegram": 0.1,
          "discord": 0.1
        },
        "update_frequency": 300,
        "sentiment_threshold": 0.6
      },
      "performance_targets": {
        "sentiment_accuracy": 0.75,
        "source_reliability": 0.80,
        "real_time_processing": true
      }
    },
    "risk_manager": {
      "id": "risk_manager_001",
      "name": "AI Risk Management System",
      "type": "risk_ml",
      "enabled": true,
      "priority": 1,
      "config": {
        "model_type": "risk_assessment",
        "risk_models": [
          "var_model",
          "cvar_model",
          "monte_carlo",
          "stress_testing"
        ],
        "risk_metrics": [
          "value_at_risk",
          "expected_shortfall",
          "maximum_drawdown",
          "sharpe_ratio",
          "sortino_ratio"
        ],
        "confidence_levels": [0.95, 0.99],
        "lookback_periods": [30, 60, 90],
        "stress_scenarios": 1000
      },
      "performance_targets": {
        "risk_prediction_accuracy": 0.85,
        "drawdown_prevention": 0.90,
        "risk_adjusted_returns": 1.5
      }
    },
    "high_frequency_trader": {
      "id": "hft_001",
      "name": "High Frequency Trading Agent",
      "type": "hft_ml",
      "enabled": true,
      "priority": 5,
      "config": {
        "model_type": "reinforcement_learning",
        "algorithm": "deep_q_network",
        "execution_speed_target": 50,
        "latency_optimization": true,
        "market_making": true,
        "arbitrage_detection": true,
        "order_book_modeling": true,
        "features": [
          "order_book_depth",
          "bid_ask_spread",
          "trade_flow",
          "market_impact",
          "liquidity_metrics"
        ]
      },
      "performance_targets": {
        "execution_speed": 50,
        "slippage_minimization": 0.001,
        "market_impact_reduction": 0.5
      }
    },
    "zero_loss_enforcer": {
      "id": "zero_loss_001",
      "name": "Zero Loss Enforcement Agent",
      "type": "protective_ml",
      "enabled": true,
      "priority": 1,
      "config": {
        "model_type": "anomaly_detection",
        "protection_algorithms": [
          "isolation_forest",
          "one_class_svm",
          "autoencoder",
          "statistical_outlier"
        ],
        "loss_threshold": 0.01,
        "hedge_ratio": 0.8,
        "emergency_stop": true,
        "real_time_monitoring": true
      },
      "performance_targets": {
        "loss_prevention_rate": 0.95,
        "false_alarm_rate": 0.05,
        "response_time": 100
      }
    },
    "ghost_trader": {
      "id": "ghost_trader_001",
      "name": "Stealth Trading Agent",
      "type": "stealth_ml",
      "enabled": true,
      "priority": 6,
      "config": {
        "model_type": "adversarial_learning",
        "stealth_algorithms": [
          "order_fragmentation",
          "timing_randomization",
          "volume_disguise",
          "pattern_obfuscation"
        ],
        "detection_avoidance": true,
        "market_impact_minimization": true
      },
      "performance_targets": {
        "stealth_effectiveness": 0.90,
        "detection_avoidance": 0.95,
        "execution_quality": 0.85
      }
    }
  },
  "training_config": {
    "data_sources": [
      "historical_market_data",
      "real_time_feeds",
      "news_data",
      "social_media_data",
      "order_book_data"
    ],
    "training_schedule": {
      "frequency": "daily",
      "time": "02:00:00",
      "duration_hours": 2
    },
    "validation": {
      "method": "time_series_split",
      "test_size": 0.2,
      "validation_size": 0.1
    },
    "model_versioning": {
      "enabled": true,
      "max_versions": 10,
      "rollback_enabled": true
    }
  },
  "deployment_config": {
    "environment": "production",
    "scaling": {
      "auto_scaling": true,
      "min_instances": 1,
      "max_instances": 5,
      "cpu_threshold": 80,
      "memory_threshold": 85
    },
    "monitoring": {
      "performance_tracking": true,
      "error_tracking": true,
      "latency_monitoring": true,
      "resource_monitoring": true
    },
    "alerts": {
      "performance_degradation": true,
      "error_rate_threshold": 0.05,
      "latency_threshold": 1000,
      "accuracy_threshold": 0.70
    }
  }
}
